{
  "private": true,
  "name": "gearbot",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "per-env",
    "start:production": "npm run -s serve",
    "start:development": "npm run -s dev",
    "build": "preact build --no-prerender --template src/index.html",
    "serve": "preact build --template src/index.html && preact serve",
    "dev": "preact watch --template src/index.html"
  },
  "devDependencies": {
    "@types/socket.io-client": "^1.4.32",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "marked": "^0.6.1",
    "node-sass": "^4.11.0",
    "preact-cli": "^3.0.0-next.19",
    "preact-render-spy": "^1.3.0",
    "sass-loader": "^7.1.0",
    "ts-loader": "^5.3.3",
    "typescript": "^3.3.4"
  },
  "dependencies": {
    "preact": "^10.0.0-alpha.2",
    "preact-markdown": "^0.4.1",
    "preact-router": "^3.0.0",
    "socket.io-client": "^2.2.0"
  }
}
